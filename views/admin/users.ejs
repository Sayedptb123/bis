<% layout('layout') %>
<h1>All Users</h1>
<table class="table table-striped">
  <thead><tr><th>Email</th><th>Role</th><th>Joined</th></tr></thead>
  <tbody>
    <% users.forEach(u=>{ %>
    <tr>
      <td><%= u.email %></td>
      <td><%= u.role %></td>
      <td><%= u.createdAt.toLocaleString() %></td>
    </tr>
    <% }) %>
  </tbody>
</table>
